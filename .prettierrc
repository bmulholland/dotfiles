{
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "printWidth": 80,
  "plugins": [
    "@prettier/plugin-lua",
    "prettier-plugin-sh",
    "prettier-plugin-toml"
  ]
}
